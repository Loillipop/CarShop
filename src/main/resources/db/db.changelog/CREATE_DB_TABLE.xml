<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

<changeSet id="createTable" author="carShopUser">
    <createTable tableName="car_shop">
        <column name="id" type="INT">
            <constraints primaryKey="true" nullable="false"/>
        </column>
        <column name="car_name" type="VARCHAR(255)">
            <constraints nullable="false"/>
        </column>
        <column name="car_model" type="VARCHAR(255)">
            <constraints nullable="false"/>
        </column>
        <column name="price" type="DECIMAL(10)">
            <constraints nullable="false"/>
        </column>
        <column name="photoUrl" type="VARCHAR(1024)">
            <constraints nullable="true"/>
        </column>

    </createTable>
</changeSet>
</databaseChangeLog>